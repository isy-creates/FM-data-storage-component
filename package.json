{
  "name": "fylo-data-storage-component",
  "version": "1.0.1",
  "description": "Practise Project from FrontendMentors",
  "main": "index.js",
  "scripts": {
    "watch:sass": "nodemon -e scss -x \"npm run compile:sass\"",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.concat.css -o css/style.prefix.css",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontendmentor-practice/fylo-data-storage-component.git"
  },
  "author": "Isy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/frontendmentor-practice/fylo-data-storage-component/issues"
  },
  "homepage": "https://github.com/frontendmentor-practice/fylo-data-storage-component#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  },
  "dependencies": {}
}
